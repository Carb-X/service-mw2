{
  "name": "service-mw2",
  "version": "1.0.0",
  "description": "Midway.js 最佳实践",
  "private": true,
  "dependencies": {
    "@midwayjs/decorator": "^2.8.11",
    "@midwayjs/orm": "^1.3.0",
    "@midwayjs/swagger": "^1.0.6",
    "@midwayjs/web": "^2.8.13",
    "@waiting/egg-jwt": "^6.0.0",
    "@waiting/shared-core": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "egg": "^2.29.3",
    "egg-koid": "^3.3.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.13.0",
    "meeko": "^1.8.156",
    "midway": "^2.8.13",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.2.51",
    "@midwayjs/egg-ts-helper": "^1.0.5",
    "@midwayjs/mock": "^2.8.13",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "cross-env": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-unicorn": "^22.0.0",
    "jest": "^26.6.3",
    "mwts": "^1.1.2",
    "swagger-ui-dist": "^3.45.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=midway-server-my_midway_project --framework=midway",
    "stop": "egg-scripts stop --title=midway-server-my_midway_project",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts",
    "debug": "cross-env NODE_ENV=local midway-bin dev --debug --ts",
    "test": "midway-bin test --ts --forceExit",
    "cov": "midway-bin cov --ts --forceExit",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "deploy": "midway-bin deploy"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-integration": {
    "lifecycle": {
      "before:package:cleanup": "npm run build"
    }
  },
  "egg": {
    "framework": "@midwayjs/web"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "anonymous",
  "license": "MIT"
}
